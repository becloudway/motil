{
    "extends": "eslint:recommended",
    "parser": "typescript-eslint-parser",
    "parserOptions": {
        "jsx": true,
        "useJSXTextNode": true
    },
    "rules": {
        "quotes": [ 2, "double" ],
        "no-undef": 1,
        "no-unused-vars": 1,
        "no-console": 1,
        "indent": [2, 4]
    }
}